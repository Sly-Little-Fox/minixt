{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "rootDir": "./src/",
    "moduleResolution": "Node",
    "outDir": "dist",
    "importsNotUsedAsValues": "remove",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
